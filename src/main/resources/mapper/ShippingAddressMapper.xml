<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tencent.mapper.ShippingAddressMapper">

    <resultMap id="BaseResultMap" type="com.tencent.model.ShippingAddress">
        <id property="id" column="id"/>
        <result property="userName" column="user_name"/>
        <result property="postalCode" column="postal_code"/>
        <result property="provinceName" column="province_name"/>
        <result property="cityName" column="city_name"/>
        <result property="countyName" column="county_name"/>
        <result property="streetName" column="street_name"/>
        <result property="detailInfo" column="detail_info"/>
        <result property="detailInfoNew" column="detail_info_new"/>
        <result property="nationalCode" column="national_code"/>
        <result property="telNumber" column="tel_number"/>
        <result property="createdAt" column="created_at"/>
        <result property="updatedAt" column="updated_at"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_name,postal_code,province_name,city_name,county_name,
        street_name,detail_info,detail_info_new,national_code,tel_number,
        created_at,updated_at
    </sql>
</mapper>
